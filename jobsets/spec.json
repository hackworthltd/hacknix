{
    "enabled": 1,
    "hidden": false,
    "description": "hacknix jobsets",
    "nixexprinput": "src",
    "nixexprpath": "jobsets/default.nix",
    "checkinterval": 60,
    "schedulingshares": 100,
    "enableemail": true,
    "emailoverride": "",
    "keepnr": 5,
    "inputs": {
         "src": { "type": "git", "value": "https://github.com/hackworthltd/hacknix.git master", "emailresponsible": false }
         ,"nixpkgs": { "type": "git", "value": "https://github.com/NixOS/nixpkgs-channels.git nixos-unstable", "emailresponsible": false }
     }
}
